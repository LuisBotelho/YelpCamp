<div class="map-container">
	<div id="map"></div>
		<script>
		  var map;
		  function initMap() {
			var center = {lat:<%=campground.coordinates.lat%>,lng: <%=campground.coordinates.lng%>};
			// The map, centered at Uluru
			var map = new google.maps.Map(document.getElementById('map'), {zoom: 14, center: center});
			// The marker, positioned at Uluru
			var marker = new google.maps.Marker({position: center, map: map});
		  }
		</script>
	<script src="https://maps.googleapis.com/maps/api/js?key=<%= gmaps_api_key %>&callback=initMap" async defer></script>	
</div>
